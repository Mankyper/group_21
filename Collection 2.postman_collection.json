							"    \"age\",\r",
							"    \"name\",\r",
							"    \"salary\"\r",
							"  ]\r",
							"    } \r",
							"    pm.response.to.have.jsonSchema(schema)\r",
							"}\r",
							");\r",
							"\r",
							"let req = request.data;\r",
							"let resp = pm.response.json();\r",
							"let salary1 = resp.salary[0]\r",
							"let salary2 = resp.salary[1]\r",
							"let salary3 = resp.salary[2]\r",
							"let salary = req.salary\r",
							"let salaryX2 = req.salary*2\r",
							"let salaryX3 = req.salary*3\r",
							"\r",
							"pm.test(\"valid_kf\", function () {\r",
							"    pm.expect(parseInt(salary)).to.eql(salary1);\r",
							"    pm.expect(salaryX2).to.eql(parseInt(salary2));\r",
							"    pm.expect(salaryX3).to.eql(parseInt(salary3));\r",
							"})\r",
							"\r",
							"pm.test(\"salary3>salary2>salary1\", function() {\r",
							"    pm.expect(parseInt(salary3)).to.be.least(parseInt(salary));\r",
							"    pm.expect(parseInt(salary)).to.be.below(parseInt(salary2));\r",
							"})\r",
							"\r",
							"\r",
							"\r",
							""
						],
